@{
Layout = "~/Views/Shared/_ManagerLayout.cshtml";
}

@model Pizzeria.Models.Item

@{
    ViewBag.Title = "Delete";
}

<h2>Delete Confirmation</h2>


<p>Are  you  sure  you  want  to  delete  <strong>@Model.ItemName</strong>? 
</p> 

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index") |   @Html.ActionLink("Log off", "ClearCookie","PizzeriaManager")
    </p>
}
